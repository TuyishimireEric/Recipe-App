<h1>New inventoryFood</h1>



<%= form_for @inventoryfood, url: inventory_inventory_food_index_url  do |form| %>
    <div>
    <%= form.label :quantity %>
    <%= form.number_field :quantity %>

      <%= form.label :food_id %>
      <%= form.select :food_id, @foods.map { |food| [food.name, food.id] } %>

    </div>
  <%= form.submit "Add Food" %>
<br>
<%end%>

<div>
  <%= link_to "Back to inventories", inventories_path %>
</div>
